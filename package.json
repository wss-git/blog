{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "cd ./release && npm install",
    "prebuild": "hexo clean && rm -rf ./public && rm -rf ./release/public",
    "build": "hexo generate",
    "postbuild": "npm run cp",
    "cp": "cp -r ./public ./release/public",
    "predeploy": "npm run build",
    "deploy": "cd release && npm run deploy",
    "pub": "cd release && npm run pub",
    "new": "hexo new post $1",
    "server": "hexo clean && hexo server",
    "publicnet": "npx lt --port 4000"
  },
  "hexo": {
    "version": "7.0.0"
  },
  "dependencies": {
    "hexo": "^7.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-search": "^2.4.3",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-stylus": "^3.0.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "hexo-wordcount": "^6.0.1"
  },
  "devDependencies": {
    "localtunnel": "^2.0.2"
  }
}
